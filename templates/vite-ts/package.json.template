  },
  "dependencies": {
    "webflow-router": "VERSION_PLACEHOLDER",
    "@vue/reactivity": "^3.x.x"
  },
  "devDependencies": {
    "vite": "^5.x.x",
    "typescript": "^5.x.x",
    "@typescript-eslint/eslint-plugin": "^7.x.x",
    "@typescript-eslint/parser": "^7.x.x",
    "eslint": "^9.28.x"
  }
}
